<Window x:Class="InvoiceManagementTool.Windows.CashierAccessWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InvoiceManagementTool.Windows"
        mc:Ignorable="d"
        Title="CashierAccessWindow" Height="120" Width="120" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <Button Name="ClientButton" Content="Add client" Width="90" HorizontalAlignment="Center" Margin="0,15,0,0" VerticalAlignment="Top" Click="ClientButton_Click"/>
            <Button Name="InvoiceButton" Content="Add invoice" Width="90" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Click="InvoiceButton_Click"/>
        </StackPanel>

    </Grid>
</Window>
